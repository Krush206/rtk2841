alias gdb2 "gdb -args tb_dvr -m VCPU -dbus_no_ovhd_timing -rbus_no_ovhd_timing  -ve3_dump 1128 -dc_map 1 -iss_t cpu.trace -ps2k -ddr3  -s ddd.scr"
